                                 ;***********************************************************
                                 ;*	Interrupt Vectors
                                 ;***********************************************************
                                 .org	$0000					; Beginning of IVs
000000 c055                      		rjmp INIT				; Reset interrupt
                                 
                                 .org	$0056					; End of Interrupt Vectors
                                 
                                 ;***********************************************************
                                 ;*	Program Initialization
                                 ;***********************************************************
                                 INIT:							; The initialization routine
                                 		; Initialize Stack Pointer
000056 ef0f                      		ldi	mpr, LOW(RAMEND)			; load low byte of RAMEND into r16
000057 bf0d                      		out	SPL, mpr					; store r16 in stack pointer low
000058 e00a                      		ldi	mpr, HIGH(RAMEND)			; load high byte of RAMEND into r16
000059 bf0e                      		out	SPH, mpr					; store r16 in stack pointer high
                                 
                                 		; Initialize LCD Display
00005a d077                      		rcall LCDInit
                                 
                                 		; Initialize Port D buttons
00005b e000                      		ldi		mpr, $00		; Set Port D Data Direction Register
00005c b90a                      		out		DDRD, mpr		; for input
00005d ef0f                      		ldi		mpr, $FF		; Initialize Port D Data Register
00005e b90b                      		out		PORTD, mpr		; so all Port D inputs are Tri-State
                                 
                                 
                                 		; NOTE that there is no RET or RJMP from INIT,
                                 		; this is because the next instruction executed is the
                                 		; first instruction of the main program
                                 
                                 ;***********************************************************
                                 ;*	Main Program
                                 ;***********************************************************
                                 MAIN:							; The Main program
                                 
                                 		; Read the value of PIND into the MPR
00005f b109                      		in mpr, PIND
                                 		
                                 		;; Check if the pin pressed was 4, active LOW, or 0b11101111
000060 3e0f                      		cpi mpr, 0b11101111
000061 f039                      		breq ClearDisplay
                                 		
                                 		;; Check if the pin pressed was 5, active LOW, or 0b11011111
000062 3d0f                      		cpi mpr, 0b11011111
000063 f041                      		breq PrintNamesOrder1
                                 		
                                 		;; Check if the pin pressed was 6, active LOW, or 0b10111111
000064 3b0f                      		cpi mpr, 0b10111111
000065 f069                      		breq PrintNamesOrder2
                                 		
                                         ;; Check if the pin pressed was 7, active LOW, or 0b01111111
000066 370f                              cpi mpr, 0b01111111
000067 f091                              breq ScrollNames
                                 		;rcall ScrollNames
                                 
000068 cff6                      		rjmp	MAIN			; jump back to main and create an infinite
                                 								; while loop.  Generally, every main program is an
                                 								; infinite while loop, never let the main program
                                 								; just run off
                                 
                                 ;***********************************************************
                                 ;*	Functions and Subroutines
                                 ;***********************************************************
                                 
                                 ;-----------------------------------------------------------
                                 ; Clears the LCD display and turns the Backlight OFF
                                 ;-----------------------------------------------------------
                                 ClearDisplay:
000069 d08c                      		rcall LCDBacklightOff
00006a d0a6                      		rcall LCDClr
00006b 9508                      		ret
                                 
                                 ;-----------------------------------------------------------
                                 ; Prints the names in the first order
                                 ;-----------------------------------------------------------
                                 PrintNamesOrder1:
                                 	; Display the strings on the LCD Display
00006c d087                      	rcall LCDBacklightOn
00006d e010                      	ldi r17, 0x00			; This will cause the lines to be switched when set to 0xFF
00006e d022                      	rcall FirstName
00006f d032                      	rcall SecondName
000070 d087                      	rcall LCDWrite
000071 940c 005f                 	jmp MAIN
                                 
                                 ;-----------------------------------------------------------
                                 ; Prints the names in the second order
                                 ;-----------------------------------------------------------
                                 PrintNamesOrder2:
                                 	; Display the strings on the LCD Display
000073 d080                      	rcall LCDBacklightOn
000074 ef1f                      	ldi r17, 0xFF			; This will cause the lines to be switched when set to 0xFF
000075 d01b                      	rcall FirstName
000076 d02b                      	rcall SecondName
000077 d080                      	rcall LCDWrite
000078 940c 005f                 	jmp MAIN
                                 
                                 ;-----------------------------------------------------------
                                 ; Scroll Names horizontally across LCD display, flipping the lines when they circle around. Do this until the button is released
                                 ;-----------------------------------------------------------
                                 ScrollNames:
00007a e1a0                              ldi XL, 0x10					; Store the end of the First Line in register Z, one past the limit cause we pre decrement
00007b e0b1                      		ldi XH, 0x01
                                 
00007c 90de                              ld r13, -X						; Load the last char of Z in r13, then decrement
                                 
00007d e2c0                              ldi YL, 0x20					; Store the end of the Second Line in register Y
00007e e0d1                              ldi YH, 0x01
                                 
00007f 918a                              ld r24, -Y                      ; Load the last char of Y in r24, then decrement
                                 
                                         ShiftChars:
000080 90fe                                  ld r15, -X                  ; Load char Z points to into r15, then decrement
000081 919a                                  ld r25, -Y					; Load char Y points to into r25
                                 
000082 9611                                  adiw XH:XL, 1					; Add 1 to Z to move the char one to the right, if the first loop we are not at the last character
000083 9621                                  adiw YH:YL, 1					; Add 1 to Y to move the char one to the right
                                 
000084 92fc                                  st X, r15                   ; Store the char from r15 back into Z. Its now shifted one right
000085 8398                                  st Y, r25                   ; Store the char from r25 back into Y. Its now shifted one right
                                 
000086 9711                                  sbiw XH:XL, 1					; Get back to the next character
000087 9721                                  sbiw YH:YL, 1					; Get back to the next character
                                 
000088 30a0                                  cpi XL,  0x00				; If the position of Z has returned to the beginning of the first line (end the loop)
000089 f7b1                                  brne ShiftChars             ; repeat the loop if Z doesn't point to the beginning yet
                                         
00008a 938c                              st X, r24                       ; Store the saved char from the end of Z (line 1) into the front of Y (line 2)
00008b 82d8                              st Y, r13                       ; Store the saved char from the end of Y (line 2) into the front of Z (line 1)
                                 
00008c e372                              ldi r23, 50                ; Load the wait time of 1 second into the wait count
00008d d025                              rCall Wait                      ; Call wait function to wait for a second
00008e d069                              rCall LCDWrite                  ; Write the adjusted line to the LCD display
                                 
00008f 940c 005f                 	jmp MAIN                             ; Return to the main
                                 
                                 ;-----------------------------------------------------------
                                 ; Prints the first name out of the program memory when R17 is set to 0xFF it will flip the lines in which the names are wrriten on
                                 ;-----------------------------------------------------------
                                 FirstName:	
                                 	; Move strings from Program Memory to Data Memory
000091 e8e4                      	ldi	ZL,LOW(2*FIRST_NAME)			; load 2*MY_NAME
000092 e0f1                      	ldi	ZH,HIGH(2*FIRST_NAME)			; into Z pointer
                                 
000093 3f1f                      	cpi r17, 0xFF						; Check if r17 is set to 0xFF if so we want to swap the names
000094 f021                      	breq first_name_line2
                                 
                                 	; Load the setup for line 1 and then skip to the loop
000095 e0c0                      	ldi	YL, 0x00						; Initialize the low byte of Y to 00
000096 e0d1                      	ldi	YH, 0x01						; Initialize the high byte of Y to 01
000097 940c 009d                 	jmp first_loop						; Go to the first loop cycle
                                 
                                 	; Move the pointer so we will write to line 2
                                 	first_name_line2:
000099 e1c0                      		ldi	YL, 0x10					; Initialize the low byte of Y to 10
00009a e0d1                      		ldi	YH, 0x01					; Initialize the high byte of Y to 01
00009b 940c 009d                 		jmp first_loop
                                 	
                                 
                                 	first_loop:
00009d 9105                      		lpm mpr, Z+						; Load program memory into the Z register
00009e 9309                      		ST Y+, mpr						; Load the mpr into the Y register
                                 
00009f 39e4                      		cpi ZL, LOW(2*FIRST_NAME_END)
0000a0 f7e1                      		brne first_loop
0000a1 9508                      	ret
                                 
                                 ;-----------------------------------------------------------
                                 ; Prints the second name out of the program memory when R17 is set to 0xFF it will flip the lines in which the names are wrriten on
                                 ;-----------------------------------------------------------
                                 SecondName:
                                 	; Move strings from Program Memory to Data Memory
0000a2 e9e4                      	ldi	ZL,LOW(2*SECOND_NAME)			; load 2*MY_NAME
0000a3 e0f1                      	ldi	ZH,HIGH(2*SECOND_NAME)			; into Z pointer
                                 
0000a4 3f1f                      	cpi r17, 0xFF						; Check if the multi-purpose register is set to 0xFF if so we want to swap the names
0000a5 f021                      	breq second_name_line1
                                 
                                 	; Load the setup for line 2 and then skip to the loop
0000a6 e1c0                      	ldi	YL, 0x10					; Initialize the low byte of Y to 10
0000a7 e0d1                      	ldi	YH, 0x01					; Initialize the high byte of Y to 01
0000a8 940c 00ae                 	jmp second_loop					; Go to the first loop cycle
                                 
                                 	; Move the Y pointer so we write to line 1
                                 	second_name_line1:
0000aa e0c0                      		ldi	YL, 0x00					; Initialize the low byte of Y to 00
0000ab e0d1                      		ldi	YH, 0x01					; Initialize the high byte of Y to 01
0000ac 940c 00ae                 		jmp second_loop
                                 
                                 	; Loop to go through the program memory until it reaches the end at which point we want to write the data to the LCD display memory
                                 	second_loop:
0000ae 9105                      		lpm mpr, Z+							; Load program memory into the Z register
0000af 9309                      		ST Y+, mpr							; Load the mpr into the Y register
                                 
0000b0 3ae4                      		cpi ZL, LOW(2*SECOND_NAME_END)		; Check if the lower byte of the Z address is equal to the lower byte of the end of the name
0000b1 f7e1                      		brne second_loop					; If not keep copying the characters to the LCD memory
0000b2 9508                      	ret
                                 
                                 ;----------------------------------------------------------------
                                 ; Sub:	Wait
                                 ; Desc:	A wait loop that is 16 + 159975*waitcnt cycles or roughly
                                 ;		waitcnt*10ms.  Just initialize wait for the specific amount
                                 ;		of time in 10ms intervals. Here is the general eqaution
                                 ;		for the number of clock cycles in the wait loop:
                                 ;			(((((3*ilcnt)-1+4)*olcnt)-1+4)*waitcnt)-1+16
                                 ;----------------------------------------------------------------
                                 Wait:
0000b3 937f                      	push	r23			; Save wait register
0000b4 932f                      	push	r18			; Save ilcnt register
0000b5 933f                      	push	r19			; Save olcnt register
                                 
0000b6 ee30                      Loop:	ldi		r19, 224		; load olcnt register
0000b7 ee2d                      OLoop:	ldi		r18, 237		; load ilcnt register
0000b8 952a                      ILoop:	dec		r18			; decrement ilcnt
0000b9 f7f1                      	brne	ILoop			; Continue Inner Loop
0000ba 953a                      	dec		r19		; decrement olcnt
0000bb f7d9                      	brne	OLoop			; Continue Outer Loop
0000bc 957a                      	dec		r23		; Decrement wait
0000bd f7c1                      	brne	Loop			; Continue Wait loop
                                 
0000be 913f                      	pop		r19		; Restore olcnt register
0000bf 912f                      	pop		r18		; Restore ilcnt register
0000c0 917f                      	pop		r23		; Restore wait register
0000c1 9508                      	ret				; Return from subroutine
                                 
                                 
