                                 ; Register definitions
                                 .def	count = r20			; Character Counter
                                 .def	q = r21			; Quotient for div10
                                 .def	r = r22			; Remander for div10
                                 
                                 ; The following constants are not intended to be modified.
                                 .equ	lcd_buffer_addr = 0x0100 ; define the SRAM base address of the LCD buffer
                                 
                                 ; This information is gathered from the ST7565R datasheet.
                                 ; define some command patterns
                                 .equ	lcd_c_disp_en = 0b10101110 ; lsb indicates whether the LCD is on (1) or off (0)
                                 .equ	lcd_c_disp_set_start_ln = 0b01000000 ; lower 6 bits indicate the line index (0 - 63)
                                 .equ	lcd_c_disp_set_page_addr = 0b10110000 ; lower 4 bits indicate the page address (page 3 is at top and page 0 is at bottom)
                                 ; together, these commands allow you to specify a column index from (0 - 131):
                                 .equ	lcd_c_disp_set_col_addr_h = 0b00010000 ; lower 4 bits indicate the 4 high bits of the column address
                                 .equ	lcd_c_disp_set_col_addr_l = 0b00000000 ; lower 4 bits indicate the 4 low bits of the column address
                                 .equ	lcd_c_disp_set_invert = 0b10100110 ; lsb indicates whether the LCD is normal (0) or inverted (1)
                                 .equ	lcd_c_disp_fill = 0b10100100 ; lsb indicates whether the LCD is normal (0) or filled with dots (1)
                                 .equ	lcd_c_disp_reset = 0b11100010 ; resets the line/page addresses and other stuff, does not affect data RAM
                                 .equ	lcd_c_disp_set_bias = 0xA2 ; 1/6 bias (in 1/33 duty mode)
                                 .equ	lcd_c_disp_set_scandir = 0b11000000 ; select normal scan direction (most significant bit is placed at top of LCD)
                                 .equ	lcd_c_disp_set_contrast_1 = 0b10000001 ; first we send this command then we pass the value
                                 .equ	lcd_c_disp_set_contrast_2 = 15 ; can select a contrast value between 1 and 63
                                 .equ	lcd_c_disp_set_res_ratio = 0x22 ; see the datasheet
                                 .equ	lcd_c_disp_vreg_on = 0x2F ; enable voltage booster, voltage regulator, and voltage follower
                                 
                                 ; use this command to write into the LCD RAM:
                                 .equ	lcd_c_disp_write = 0b00000000 ; these 8 bits are the data to be placed in the target. 
                                 ; note that the column address is automatically incremented by 1 after each write to the LCD data
                                 
                                 ; everything in this driver file needs to go into the code segment
                                 .cseg
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDInit
                                 ;* Desc:	This function initializes the LCD control pins and
                                 ;*		sets reasonable values for the configuration registers.
                                 ;*****************************************************************
                                 LCDInit:
0000d2 930f                      	push	mpr
                                 
                                 	; set MOSI, SCL, and SS as outputs
0000d3 9a22                      	sbi	DDRB, DDB2	; Port B 2
0000d4 9a21                      	sbi	DDRB, DDB1	; Port B 1
0000d5 9a20                      	sbi	DDRB, DDB0	; Port B 0
                                 	; ensure that SS is deactivated (active low)
0000d6 9a28                      	sbi	PORTB, 0
                                 	; set lcd_A0 as output
0000d7 9a81                      	sbi	DDRF, DDF1	; Port F 1
                                 	; enable LCD backlight control
0000d8 9a3f                      	sbi	DDRC, DDC7	; Port C 7
                                 	; set lcd_RST_N as output
0000d9 9a80                      	sbi	DDRF, DDF0	; Port F 0
                                 	; we pull lcd_RST_N low for 1 millisecond
0000da 9888                      	cbi	PORTF, 0
0000db 940e 01bd                 	call	LCDDelay
0000dd 9a88                      	sbi	PORTF, 0
                                 	; Enable SPI mode 3, master configuration, set clock rate fck/2 using SPI2X
0000de e50c                      	ldi	mpr, (1<<SPE)|(1<<MSTR)|(1<<CPOL)|(1<<CPHA)
0000df bd0c                      	out	SPCR, mpr
0000e0 e001                      	ldi	mpr, (1<<SPI2X)
0000e1 bd0d                      	out	SPSR, mpr
                                 	; Note: after transmitting a byte, the SPI interrupt flag will
                                 	; be automatically cleared next time we: 1) read SPSR and ALSO 2) access SPDR
                                 
0000e2 9828                      	cbi	PORTB, 0	; activate slave select
                                 	; send a string of initialization commands
0000e3 ea02                      	ldi	mpr, lcd_c_disp_set_bias
0000e4 d0d2                      	rcall	LCD_Internal_WriteCMD
0000e5 ec00                      	ldi	mpr, lcd_c_disp_set_scandir
0000e6 d0d0                      	rcall	LCD_Internal_WriteCMD
0000e7 e801                      	ldi	mpr, lcd_c_disp_set_contrast_1
0000e8 d0ce                      	rcall	LCD_Internal_WriteCMD
0000e9 e00f                      	ldi	mpr, lcd_c_disp_set_contrast_2
0000ea d0cc                      	rcall	LCD_Internal_WriteCMD
0000eb e202                      	ldi	mpr, lcd_c_disp_set_res_ratio
0000ec d0ca                      	rcall	LCD_Internal_WriteCMD
0000ed e20f                      	ldi	mpr, lcd_c_disp_vreg_on
0000ee d0c8                      	rcall	LCD_Internal_WriteCMD
0000ef ea0f                      	ldi	mpr, (lcd_c_disp_en | 1 << 0)
0000f0 d0c6                      	rcall	LCD_Internal_WriteCMD
0000f1 9a28                      	sbi	PORTB, 0	; deactivate slave select
                                 
0000f2 910f                      	pop	mpr
0000f3 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDBacklightOn
                                 ;* Desc:	Enables the backlight
                                 ;*****************************************************************
                                 LCDBacklightOn:
0000f4 9a47                      	sbi	PORTC, 7
0000f5 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDBacklightOff
                                 ;* Desc:	Disables the backlight
                                 ;*****************************************************************
                                 LCDBacklightOff:
0000f6 9847                      	cbi	PORTC, 7
0000f7 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrite
                                 ;* Desc:	This function writes 32 characters to the LCD screen...
                                 ;*		16 chars on the top line and 16 chars on the bottom line.
                                 ;*****************************************************************
                                 LCDWrite:
0000f8 d002                      	rcall	LCDWrLn1
0000f9 d00c                      	rcall	LCDWrLn2
0000fa 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrLn1
                                 ;* Desc:	This function writes 16 characters to the top line.
                                 ;*****************************************************************
                                 LCDWrLn1:
0000fb 93bf                      	push	XH
0000fc 93af                      	push	XL
0000fd 931f                      	push	r17
                                 	; r17 specifies the destination bank in LCD RAM
0000fe e013                      	ldi	r17, 3
                                 	; point X at the ASCII data
0000ff e0b1                      	ldi	XH, HIGH(lcd_buffer_addr)
000100 e0a0                      	ldi	XL, LOW(lcd_buffer_addr)
000101 d05c                      	rcall	LCD_Internal_WriteLn
000102 911f                      	pop	r17
000103 91af                      	pop	XL
000104 91bf                      	pop	XH
000105 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDWrLn2
                                 ;* Desc:	This function writes 16 characters to the bottom line.
                                 ;*****************************************************************
                                 LCDWrLn2:
000106 93bf                      	push	XH
000107 93af                      	push	XL
000108 931f                      	push	r17
                                 	; r17 specifies the destination bank in LCD RAM
000109 e011                      	ldi	r17, 1
                                 	; point X at the ASCII data
00010a e0b1                      	ldi	XH, HIGH(lcd_buffer_addr+16)
00010b e1a0                      	ldi	XL, LOW(lcd_buffer_addr+16)
00010c d051                      	rcall	LCD_Internal_WriteLn
00010d 911f                      	pop	r17
00010e 91af                      	pop	XL
00010f 91bf                      	pop	XH
000110 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDClr
                                 ;* Desc:	This function clears the entire LCD screen.
                                 ;*****************************************************************
                                 LCDClr:
000111 d022                      	rcall	LCDClrLn1
000112 d027                      	rcall	LCDClrLn2
                                 
000113 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	CLRDM1
                                 ;* Desc:	This function clears Data Memory by writing 16 
                                 ;*		consecutive ASCII spaces $20 to the memory at $0100.
                                 ;*****************************************************************
                                 CLRDM1:
                                     ; Set X register points to $0100
000114 93bf                      	   push	   XH
000115 93af                      	   PUSH	   XL
000116 930f                                 push    r16
000117 931f                                 push    r17
                                 
000118 e0b1                                 ldi     XH, HIGH(lcd_buffer_addr)
000119 e0a0                                 ldi     XL, LOW(lcd_buffer_addr)
00011a e110                                 ldi     r17, 16
00011b e200                                 ldi     r16, ' '
                                    CLRloop1:
00011c 930d                                 st      X+, r16         ; load data to DM
00011d 951a                                 dec     r17
00011e f7e9                                 brne    CLRloop1
                                 
00011f 911f                      	   pop	   r17
000120 910f                      	   pop	   r16
000121 91af                      	   pop	   XL
000122 91bf                      	   pop	   XH
                                 
000123 9508                                 ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	CLRDM2
                                 ;* Desc:	This function clears Data Memory by writing 16 
                                 ;*		consecutive ASCII spaces $20 to the memory at $0110.
                                 ;*****************************************************************
                                 CLRDM2:
                                     ; Set X register points to $0110
000124 93bf                      	   push	   XH
000125 93af                      	   PUSH	   XL
000126 930f                                 push    r16
000127 931f                                 push    r17
                                 
000128 e0b1                                 ldi     XH, HIGH(lcd_buffer_addr+16)
000129 e1a0                                 ldi     XL, LOW(lcd_buffer_addr+16)
00012a e110                                 ldi     r17, 16
00012b e200                                 ldi     r16, ' '
                                    CLRloop2:
00012c 930d                                 st      X+, r16         ; load data to DM
00012d 951a                                 dec     r17
00012e f7e9                                 brne    CLRloop2
                                 
00012f 911f                      	   pop	   r17
000130 910f                      	   pop	   r16
000131 91af                      	   pop	   XL
000132 91bf                      	   pop	   XH
                                 
000133 9508                                 ret
                                 ;*****************************************************************
                                 ;* Func:	LCDClrLn1
                                 ;* Desc:	This function clears the top line of the LCD screen.
                                 ;*****************************************************************
                                 LCDClrLn1:
000134 931f                      	push	r17
                                 	; r17 specifies the target bank in LCD RAM
000135 e013                      	ldi	r17, 3
000136 d060                      	rcall	LCD_Internal_ClearLn
000137 dfdc                      	rcall	CLRDM1
000138 911f                      	pop	r17
000139 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDClrLn2
                                 ;* Desc:	This function clears the bottom line of the LCD screen.
                                 ;*****************************************************************
                                 LCDClrLn2:
00013a 931f                      	push	r17
                                 	; r17 specifies the target bank in LCD RAM
00013b e011                      	ldi	r17, 1
00013c d05a                      	rcall	LCD_Internal_ClearLn
00013d dfe6                      	rcall	CLRDM2
00013e 911f                      	pop	r17
00013f 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	Bin2ASCII
                                 ;* Desc:	Converts a binary number into an ASCII 
                                 ;*			text string equivalent. 
                                 ;*			- The binary number needs to be in the mpr
                                 ;*			- The Start Address of where the text will
                                 ;*			 	be placed needs to be in the X Register
                                 ;*			- The count of the characters created are 
                                 ;*				added to the count register
                                 ;*****************************************************************
                                 Bin2ASCII:
000140 930f                      	push	mpr			; save mpr
000141 936f                      	push	r			; save r
000142 935f                      	push	q			; save q
000143 93bf                      	push	XH			; save X-pointer
000144 93af                      	push	XL			;
                                 
                                 	; Determine the range of mpr
000145 3604                      	cpi	mpr, 100		; is mpr >= 100
000146 f018                      	brlo	B2A_1			; goto next check
000147 e043                      	ldi	count, 3		; Three chars are written
000148 9613                      	adiw	XL, 3			; Increment X 3 address spaces
000149 c007                      	rjmp	B2A_3			; Continue with program
                                 B2A_1:
00014a 300a                      	cpi	mpr, 10		; is mpr >= 10
00014b f018                      	brlo	B2A_2			; Continue with program
00014c e042                      	ldi	count, 2		; Two chars are written
00014d 9612                      	adiw	XL, 2			; Increment X 2 address spaces
00014e c002                      	rjmp	B2A_3			; Continue with program
                                 B2A_2:
00014f 9611                      	adiw	XL, 1			; Increment X 1 address space
000150 e041                      	ldi	count, 1		; One char is written
                                 
                                 B2A_3:
                                 	; Do-While statement that converts Binary to ASCII
000151 d076                      	rcall	div10			; Call the div10 function
000152 e300                      	ldi	mpr, '0'		; Set the base ASCII integer value
000153 0f06                      	add	mpr, r		; Create the ASCII integer value
000154 930e                      	st	-X, mpr		; Load ASCII value to memory
000155 2f05                      	mov	mpr, q		; Set mpr to quotient value
000156 3000                      	cpi	mpr, 0		; does mpr == 0
000157 f7c9                      	brne	B2A_3			; do while (mpr != 0)
                                 
000158 91af                      	pop	XL			; restore X-pointer
000159 91bf                      	pop	XH			;
00015a 915f                      	pop	q			; restore q
00015b 916f                      	pop	r			; restore r
00015c 910f                      	pop	mpr			; restore mpr
00015d 9508                      	ret				; return from function
                                 
