                                 ;*	The functions below here are intended for internal use only.
                                 ;*****************************************************************
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteLn
                                 ;* Desc:	Sends 16 characters of information to the LCD panel
                                 ;*		This is slightly more complicated than it sounds because the
                                 ;*		characters straddle two banks of the LCD memory.
                                 ;*		X is pointing at the first ASCII character to be displayed.
                                 ;*		r17 holds the index of the destination bank in LCD RAM.
                                 ;*		
                                 ;*****************************************************************
                                 LCD_Internal_WriteLn:
00015e 920f                      	push	r0
00015f 921f                      	push	r1
000160 930f                      	push	mpr
000161 932f                      	push	r18
000162 933f                      	push	r19
000163 938f                      	push	r24
000164 939f                      	push	r25
000165 93ef                      	push	ZL
000166 93ff                      	push	ZH
                                 
                                 	; r25:24 holds a pointer to the base address of the bitmap array
000167 e093                      	ldi	r25, HIGH(FontBitmap << 1);
000168 ed8c                      	ldi	r24, LOW(FontBitmap << 1);
                                 
000169 9828                      	cbi	PORTB, 0	; activate slave select
                                 
                                 	; use a counter to track if we are on the top chunk or bottom chunk of each char
00016a e022                      	ldi	r18, 2
                                 
                                 LCD_WL_display_bank:
                                 	; set the column to 0
00016b e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
00016c d04a                      	rcall	LCD_Internal_WriteCMD
00016d e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
00016e d048                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
00016f eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
000170 2b01                      	or	mpr, r17	; set the lower 4 bits
000171 d045                      	rcall	LCD_Internal_WriteCMD
                                 
000172 e130                      	ldi	r19, 16	; there are 16 characters per line
                                 
                                 LCD_WL_next_char:
                                 	; get ascii code point & determine location of corresponding character bitmap
000173 900d                      	ld	r0, X+	; this should be a value from 0 to 255
000174 e100                      	ldi	mpr, 16	; there are 16 bytes per character
000175 9e00                      	mul	r0, mpr	; determine the byte offset for the desired char
000176 01fc                      	movw	ZH:ZL, r25:r24	; copy the base address to Z
000177 0de0                      	add	ZL, r0	; finally, compute a pointer to the char bitmap
000178 1df1                      	adc	ZH, r1
                                 
000179 e008                      	ldi	mpr, 8	; there are 8 columns for each character
                                 
                                 	; this is where we display the chunk of character (either a top chunk or bottom chunk)
00017a 9004                      	lpm	r0, Z
                                 LCD_WL_next_col:
00017b bc0e                      	out	SPDR, r0
00017c 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
00017d 9632                      	adiw	ZH:ZL, 2	; point Z at the next column for this bank
00017e 9004                      	lpm	r0, Z		; this is a sneaky way to save a few clock cycles (since we are waiting for the SPI communication)
                                 LCD_WL_wait_spi:
00017f b41d                      	in	r1, SPSR
000180 fe17                      	sbrs	r1, SPIF	; wait for SPI to finish
000181 cffd                      	rjmp	LCD_WL_wait_spi
                                 
                                 	; keep going until we have displayed all the columns for this character
000182 950a                      	dec	mpr
000183 f7b9                      	brne	LCD_WL_next_col
                                 
                                 	; move to the next character
000184 953a                      	dec	r19
000185 f769                      	brne	LCD_WL_next_char
                                 
                                 	; if we are at the end of the line, configure the bank and starting array offset for the bottom half of each char
000186 952a                      	dec	r18		; did we display both banks yet?
000187 f021                      	breq	LCD_WL_all_done
                                 	; if not, then prepare to display the bottom chunk of each character
000188 951a                      	dec	r17		; decrement the bank number
000189 9750                      	sbiw	XH:XL, 16	; X needs to point back at the beginning character of the line
00018a 9601                      	adiw	r25:r24, 1	; increment the base address pointer (now it will point to the second chunk of each char)
00018b cfdf                      	rjmp	LCD_WL_display_bank
                                 
                                 LCD_WL_all_done:
00018c 9a28                      	sbi	PORTB, 0	; deactivate slave select
                                 
00018d 91ff                      	pop	ZH
00018e 91ef                      	pop	ZL
00018f 919f                      	pop	r25
000190 918f                      	pop	r24
000191 913f                      	pop	r19
000192 912f                      	pop	r18
000193 910f                      	pop	mpr
000194 901f                      	pop	r1
000195 900f                      	pop	r0
000196 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_ClearLn
                                 ;* Desc:	This function clears two of the LCD memory banks.
                                 ;*		r17 specifies the starting bank to wipe.
                                 ;*****************************************************************
                                 LCD_Internal_ClearLn:
000197 930f                      	push	mpr
000198 921f                      	push	r1
000199 932f                      	push	r18
00019a 933f                      	push	r19
                                 
00019b 9828                      	cbi	PORTB, 0	; activate slave select
00019c e032                      	ldi	r19, 2	; we are going to clear two banks
                                 
                                 LCD_CLRLN_clear_bank:
                                 	; set the column to 0
00019d e100                      	ldi	mpr, lcd_c_disp_set_col_addr_h
00019e d018                      	rcall	LCD_Internal_WriteCMD
00019f e000                      	ldi	mpr, lcd_c_disp_set_col_addr_l
0001a0 d016                      	rcall	LCD_Internal_WriteCMD
                                 	; we need to select the appropriate page
0001a1 eb00                      	ldi	mpr, lcd_c_disp_set_page_addr
0001a2 2b01                      	or	mpr, r17	; configure the lower 4 bits
0001a3 d013                      	rcall	LCD_Internal_WriteCMD
                                 
0001a4 e820                      	ldi	r18, 128	; there are 128 columns on the LCD
0001a5 2700                      	clr	mpr		; we will fill each column with empty dots (i.e. blank space)
0001a6 9a89                      	sbi	PORTF, 1	; set lcd_A0 to specify data mode
                                 
                                 LCD_CLRLN_write_col:
0001a7 bd0e                      	out	SPDR, mpr	; send an empty column to the LCD via SPI
                                 LCD_CLRLN_wait_spi:
0001a8 b41d                      	in	r1, SPSR
0001a9 fe17                      	sbrs	r1, SPIF	; wait for SPI to finish
0001aa cffd                      	rjmp	LCD_CLRLN_wait_spi
                                 
0001ab 952a                      	dec	r18
0001ac f7d1                      	brne	LCD_CLRLN_write_col	; cycle through all 128 columns
                                 
                                 	; clear the remaining bank if we haven't done so already
0001ad 953a                      	dec	r19
0001ae f011                      	breq	LCD_CLRLN_all_done
0001af 951a                      	dec	r17		; decrement the bank index
0001b0 cfec                      	rjmp	LCD_CLRLN_clear_bank
                                 
                                 LCD_CLRLN_all_done:
0001b1 9a28                      	sbi	PORTB, 0	; deactivate slave select	
                                 	
                                 	; pop registers...
0001b2 913f                      	pop	r19
0001b3 912f                      	pop	r18
0001b4 901f                      	pop	r1
0001b5 910f                      	pop	mpr
0001b6 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCD_Internal_WriteCMD
                                 ;* Desc:	Sends the byte in mpr
                                 ;*		If you use this function directly,
                                 ;*		YOU ARE RESPONSIBLE for handling the slave select line!
                                 ;*****************************************************************
                                 LCD_Internal_WriteCMD:
0001b7 bd0e                      	out	SPDR, mpr	; initiate transmission
0001b8 9889                      	cbi	PORTF, 1	; clear lcd_A0 to put the lcd into command mode
                                 				; (this just has to be done before the 8th bit
                                 				; is transmitted via SPI)
                                 LCD_IWC_L1:
0001b9 b50d                      	in	mpr, SPSR
0001ba ff07                      	sbrs	mpr, SPIF	; wait for SPI to finish
0001bb cffd                      	rjmp LCD_IWC_L1
0001bc 9508                      	ret
                                 
                                 ;*****************************************************************
                                 ;* Func:	LCDDelay
                                 ;* Desc:	A wait loop that will consume roughly 1ms
                                 ;*		Note that a CALL takes 4 cycles on top of this delay.
                                 ;*****************************************************************
                                 LCDDelay:
0001bd 930f                      	push	mpr		; 2 cycles
0001be 931f                      	push	r17		; 2 cycles
0001bf e01b                      	ldi	r17, 11	; 1 cycle
                                 LCDDelay_L1:
0001c0 ef02                      	ldi	mpr, 242	; 1 cycle
                                 LCDDelay_L2:
0001c1 950a                      	dec	mpr		; 1 cycle
0001c2 f7f1                      	brne	LCDDelay_L2	; 2 cycles on branch, 1 if no branch
0001c3 951a                      	dec	r17		; 1 cycle
0001c4 f7d9                      	brne	LCDDelay_L1	; 2 cycles on branch, 1 if no branch
0001c5 911f                      	pop	r17		; 2 cycles
0001c6 910f                      	pop	mpr		; 2 cycles
0001c7 9508                      	ret			; 4 cycles
                                 
                                 ;*****************************************************************
                                 ;* Func:	div10
                                 ;* Desc:	Divides the value in the mpr by 10 and 
                                 ;*			puts the remainder in the 'r' register
                                 ;*			and the quotient in the 'q' register.
                                 ;*	DO NOT modify this function, trust me, it does
                                 ;*	divide by 10 :)  ~DZ		
                                 ;*****************************************************************
                                 div10:
0001c8 920f                      	push	r0		; Save register
                                 
                                 	; q = mpr / 10 = mpr * 0.000110011001101b
0001c9 2f50                      	mov	q, mpr	; q = mpr * 1.0b
0001ca 9556                      	lsr	q		; q >> 2
0001cb 9556                      	lsr	q		; q = mpr * 0.01b
0001cc 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001cd 9556                      	lsr	q		; q = mpr * 0.101b
0001ce 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
0001cf 9556                      	lsr	q
0001d0 9556                      	lsr	q
0001d1 9556                      	lsr	q		; q = mpr * 0.001101b
0001d2 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001d3 9556                      	lsr	q		; q = mpr * 0.1001101b
0001d4 0f50                      	add	q, mpr	; q = (q + mpr) >> 3
0001d5 9556                      	lsr	q				
0001d6 9556                      	lsr	q
0001d7 9556                      	lsr	q		; q = mpr * 0.0011001101b
0001d8 0f50                      	add	q, mpr	; q = (q + mpr) >> 1
0001d9 9556                      	lsr	q		; q = mpr * 0.10011001101b
0001da 0f50                      	add	q, mpr	; q = (q + mpr) >> 4
0001db 9556                      	lsr	q
0001dc 9556                      	lsr	q
0001dd 9556                      	lsr	q
0001de 9556                      	lsr	q		; q = mpr * 0.000110011001101b
                                 
                                 	; compute the remainder as r = i - 10 * q
                                 	; calculate r = q * 10 = q * 1010b
0001df 2f65                      	mov	r, q		; r = q * 1
0001e0 0f66                      	lsl	r		; r << 2
0001e1 0f66                      	lsl	r		; r = q * 100b
0001e2 0f65                      	add	r, q		; r = (r + q) << 1
0001e3 0f66                      	lsl	r		; r = q * 1010b
0001e4 2e06                      	mov	r0, r		; r0 = 10 * q
0001e5 2f60                      	mov	r, mpr	; r = mpr
0001e6 1960                      	sub	r, r0		; r = mpr - 10 * q
                                 
                                 	; Fix any errors that occur
                                 div10_1:
0001e7 306a                      	cpi	r, 10		; Compare with 10
0001e8 f018                      	brlo	div10_2	; do nothing if r < 10
0001e9 9553                      	inc	q		; fix quotient
0001ea 506a                      	subi	r, 10		; fix remainder
0001eb cffb                      	rjmp	div10_1	; Continue until error is corrected
                                 
                                 div10_2:
0001ec 900f                      	pop	r0		; Restore registers
0001ed 9508                      	ret			; Return from function
                                 
